<link rel="stylesheet" href="~/Content/css/early-wage-user.css">

<script src="~/Content/js/Transactions/transactions.js"></script>

<div class="main_div">
    <div class="sec_div">
        <div class="drop_down" style="justify-content: flex-end;">
           @* <div class="drop_select">
                <span class="status-text">Status</span>
                <select name="cars" id="cars" class="transaction-status-select-box">
